{
  "name": "tiny-repl",
  "version": "0.1.0",
  "description": "tiny repl",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "node dist/bin/server.js",
    "build-backend": "NODE_ENV=production babel server --out-dir dist --source-maps inline",
