  "license": "MIT",
  "bugs": {
    "url": "https://github.com/corsicanec82/tiny-repl/issues"
  },
  "homepage": "https://github.com/corsicanec82/tiny-repl#readme",
  "dependencies": {
    "fastify": "^2.13.0",
    "fastify-static": "^2.6.0",
    "point-of-view": "^3.8.0",
    "pug": "^2.0.4"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/node": "^7.8.7",
    "@babel/preset-env": "^7.9.5",
    "babel-eslint": "^10.1.0",
